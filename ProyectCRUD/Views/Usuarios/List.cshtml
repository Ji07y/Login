@{
}
@model List<ProyectCRUD.Models.Entities.Usuarios>

    <h1>Usuarios</h1>
    
    <table class="table">
      
        <thead>
            
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th> </th>
            </tr>
        </thead> 
        
        <tbody>
            @foreach(var usuarios in Model)
            {
              <tr>
                <td>@usuarios.Id</td>
                <td>@usuarios.usuario</td>
                <td>@usuarios.clave</td>
                
              <td>
                 <a class="btn btn-primary" asp-asp-controller="Usuarios" asp-action="Edit" asp-route-id="@usuarios.Id">Edit</a>
               </td>
              </tr>

        }
        </tbody>
        </table>
           
